<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="SetMessageIdCookie">
    <DisplayName>SetMessageIdCookie</DisplayName>
    <Properties/>
    <Set>
        <Headers>
            <Header name="Set-Cookie">messageId={messageid}; Path=/</Header>
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>